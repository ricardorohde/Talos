  object qrCaixas: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      
        'select caixas.codigo, usuarios.nome from c000045 caixas, c000008' +
        ' usuarios'
      'where caixas.codfuncionario = usuarios.codigo'
      'group by caixas.codigo, usuarios.nome')
    Params = <>
    Left = 416
    Top = 304
    object qrCaixasCODIGO: TWideStringField
      FieldName = 'CODIGO'
      Required = True
      Size = 6
    end
    object qrCaixasNOME: TWideStringField
      FieldName = 'NOME'
      Size = 80
    end
  end
  object fsCaixas: TfrxDBDataset
    UserName = 'fsCaixas'
    CloseDataSource = False
    FieldAliases.Strings = (
      'CODIGO=CODIGO'
      'NOME=NOME')
    DataSet = qrCaixas
    BCDToCurrency = False
    Left = 416
    Top = 256
  end
  object qrVenda_Detalhado: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      
        'select nfce.CODVENDEDOR, SUM(nfce.TOTAL) TOTAL_GERAL from c00004' +
        '8 nfce '
      'GROUP BY CODVENDEDOR'
      'union'
      
        'select nfe.CODVENDEDOR, SUM(nfe.TOTAL_NOTA) TOTAL_GERAL from c00' +
        '0061 nfe '
      'where NFE_SITUACAO = 6  GROUP BY CODVENDEDOR')
    Params = <>
    Left = 384
    Top = 368
  end
