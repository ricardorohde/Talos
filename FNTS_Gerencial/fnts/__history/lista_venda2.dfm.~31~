  object qrvenda_produto3: TZQuery
    Connection = frmmodulo.Conexao
    SQL.Strings = (
      'select c000032.*,'
      
        '  case (select fun.comissao from c000008 fun where fun.codigo = ' +
        'c000032.codvendedor) when 0 then'
      
        '    (select pro.comissao from c000025 pro where pro.codigo = c00' +
        '0032.codproduto)'
      '   else'
      
        '    (select fun.comissao from c000008 fun where fun.codigo = c00' +
        '0032.codvendedor)'
      '  end comissao '
      'from c000032  WHERE movimento in (2,7,9) order by data, codIGO')
    Params = <>
    Left = 194
